    <script>

        $(document).ready(function () {



            $(".nav-sidebar > li > a").click(function () {

                if (false == $(this).next().is(':visible')) {
                    $('#accordion ul').slideUp(300);
                }
                $(this).next().slideToggle(300);
            });

            $(".nav-sidebar > li > ul > li  a").click(function () {

                localStorage.setItem('childId', $(this).parent().eq(0).attr('id'));
                localStorage.setItem('parentId', $(this).parent().eq(0).parent().eq(0).parent().eq(0).attr('id'));

            });

            $('#accordion ul:eq(0)').show();

            $('.small-menu-click ').click(function () {
                $('.nav-sidebar').toggle();
            });

            $('.navbar-toggle').click(function () {
                $('.navbar-collapse').toggle();
            });
        });

    </script>

